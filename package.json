{
  "name": "playwright-agent-tests",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "pw:install": "PLAYWRIGHT_BROWSERS_PATH=.playwright-browsers playwright install",
    "test": "PLAYWRIGHT_BROWSERS_PATH=.playwright-browsers playwright test",
    "bddgen": "playwright-bdd",
    "test:ui": "PLAYWRIGHT_BROWSERS_PATH=.playwright-browsers playwright test --ui",
    "test:ui:regen": "npx bddgen && PLAYWRIGHT_BROWSERS_PATH=.playwright-browsers playwright test --ui"
  },
  "devDependencies": {
    "@playwright/test": "1.58.1",
    "@zerostep/playwright": "latest",
    "dotenv": "latest",
    "playwright-bdd": "^8.4.2",
    "ts-node": "^10.9.2",
    "typescript": "latest"
  }
}
